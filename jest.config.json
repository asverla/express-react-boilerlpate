{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "ui/**/*.js",
    "!**/node_modules/**",
    "!**/vendor/**",
    "!**/public/**"
  ],
  "moduleNameMapper": {
    ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/ui/tests/fileMock.js",
    ".*\\.(css|less)$": "identity-obj-proxy"
  },
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules",
    "<rootDir>/public",
    "<rootDir>/ui/images",
    "<rootDir>/ui/styles"
  ]
}
