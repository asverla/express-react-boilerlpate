{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "ui/**/*.js",
    "!**/node_modules/**",
    "!**/vendor/**",
    "!**/public/**"
  ],
  "moduleNameMapper": {
    ".*\\.(css|less)$": "identity-obj-proxy"
  },
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules",
    "<rootDir>/public",
    "<rootDir>/ui/images",
    "<rootDir>/ui/styles"
  ]
}
